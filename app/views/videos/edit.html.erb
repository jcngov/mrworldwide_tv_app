<h1> Edit Page </h1>

<%= form_for @video do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :released_on %>
  <%= f.date_select :released_on %>
  <br>
  <%= f.label :embed_url, "YouTube ID" %>
  <%= f.text_field :embed_url %>
<%= f.submit "Submit Changes" %>
<% end %>


<%= link_to "Delete Video", @video, method: :delete, data: {confirm: "You Sure?"} %>

